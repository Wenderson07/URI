#include <stdio.h>

int main()
{
	int repetir,i;
	char words[20];
	scanf("%d",&repetir);
	for(i = 0;i< repetir;i++){	
	   	scanf("%s",&words);
		if(words[0] == 'o'){
			if(words[1] == 'w'){
				if(words[2] == 'e'){
				   	printf("1\n");
			}
		}
			if(words[0] == 't'){
				if(words[1] == 'h'){
					if(words[2] == 'e'){
						if(words[3] == 'e'){
							if(words[4] == 'e'){
					   	   	   	printf("3\n");
				}
				}
				}
				}
			}
			if(words[0] == 't'){
				   	if(words[1] == 'o'){
						if(words[2] == 'o'){
					printf("2\n");
			}
			}
			}
		}
	}
	return 0;
}
