#include <stdio.h>
int main(){
	int x,i,positive=0,negative=0,even=0;
	for (i =0;i <5;i++){
		scanf("%d",&x);
		if(x>0){
			positive++;
		}
		if(x<0){
			negative++;
		}
		if(x%2 == 0){
			even++;			
		}
	}
	printf("%d valor(es) par(es)\n",even);
	printf("%d valor(es) impar(es)\n",5 - even);
	printf("%d valor(es) positivo(s)\n",positive);
	printf("%d valor(es) negativo(s)\n",negative);
	
	return 0;
}
